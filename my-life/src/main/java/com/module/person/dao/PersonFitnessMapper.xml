<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.module.person.dao.PersonFitnessMapper">

        <!-- 通用查询映射结果 -->
        <resultMap id="BaseResultMap" type="com.module.person.po.PersonFitness">
                    <id column="id" property="id"/>
                    <result column="time" property="time"/>
                    <result column="diet" property="diet"/>
                    <result column="exercise" property="exercise"/>
                    <result column="version" property="version"/>
        </resultMap>

        <!-- 通用查询结果列 -->
        <sql id="Base_Column_List">
            id, time, diet, exercise, version
        </sql>

</mapper>